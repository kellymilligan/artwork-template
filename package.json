{
  "name": "artwork-template",
  "version": "1.0.0",
  "description": "GitHub template for creating artwork using canvas-sketch",
  "main": "index.js",
  "scripts": {
    "sketch": "canvas-sketch input/artwork.js --output=./output",
    "sketch-to-png": "npm run sketch",
    "sketch-to-gif": "canvas-sketch input/artwork.js --output=./output --stream [ gif --scale=512:-1 ]",
    "sketch-to-mp4": "canvas-sketch input/artwork.js --output=./output --stream [ mp4 --scale=1280:-1 ]"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kellymilligan/artwork-2020-01.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kellymilligan/artwork-2020-01/issues"
  },
  "homepage": "https://github.com/kellymilligan/artwork-2020-01#readme",
  "dependencies": {
    "canvas-sketch": "^0.7.5",
    "canvas-sketch-util": "^1.10.0"
  }
}
